require diksam.lang;

public class Point {
    private double x;
    private double y;

    // 构造函数
    constructor init(double x, double y) {
        this.x = x;
        this.y = y;
        println(this.x);
    }

    double get_x() {
        return this.x;
    }

    void set_x(double x) {
        this.x = x;
    }

    double get_y() {
        return this.y;
    }

    void set_y(double y) {
        this.y = y;
    }

    void print() {
        println("x.." + this.x + ", y.." + this.y);
    }
}


Point point = new Point(10, 20);
point.print();